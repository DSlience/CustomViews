<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--SeekBar选择控件-->
    <declare-styleable name="RangeSeekBar">
        <attr name="absoluteMinValue" format="integer|float" />
        <attr name="absoluteMaxValue" format="integer|float" />
    </declare-styleable>

    <!--跑马灯控件-->
    <declare-styleable name="MarqueeView">
        <attr name="marqueeInterval" format="integer" />
        <attr name="marqueeAnimDuration" format="integer" />
        <attr name="marqueeAnimIn" format="reference" />
        <attr name="marqueeAnimOut" format="reference" />
    </declare-styleable>

</resources>